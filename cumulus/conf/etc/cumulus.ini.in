[cb]

installdir = @INSTALLDIR@
port = 8888
hostname = @HOSTNAME@
calcMD5=True


[backend]
type=posix
data_dir=@INSTALLDIR@/posixdata
block_size=524288
[security]
type=authz
security_dir=@INSTALLDIR@/posixauth
authzdb=@AUTHZDB@


[log]
level=INFO
file=@INSTALLDIR@/log/cumulus.log

[https]
enabled=False
#key=@@KEYFILE@@
#cert=@@CERT_FILE

